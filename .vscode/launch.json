{
  "version": "0.2.0",
  "configurations": [

    {
      "name": "Flutter",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "Flutter Attach",
      "request": "attach",
      "type": "dart"
    },
    {
      "name": "Flutter Web",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-port",
        "8000"
      ]
    },

    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "e:/Alias/alias",
      "program": "e:/Alias/alias/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [{
        "description": "Enable pretty-printing for gdb",
        "text": "-enable-pretty-printing",
        "ignoreFailures": true
      }]
    }
  ]
}